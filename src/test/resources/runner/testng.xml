<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OMNI_Automation">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.omni.utilities.CustomListeners" />
	</listeners>
	<test thread-count="1" name="Login Test">
		<classes>
			<class name="com.omni.testcases.LoginTest" />
		</classes>
	</test>
	<test thread-count="1" name="PGR Test">
		<classes>
			<class name="com.omni.testcases.PgrTest" />
		</classes>
	</test>
	<test thread-count="1" name="20Factor Test">
		<classes>
			<class name="com.omni.testcases.Factor20Test" />
		</classes>
	</test>
	<test thread-count="1" name="My Chaikin Test">
		<classes>
			<class name="com.omni.testcases.MyChaikinTest" />
		</classes>
	</test>
</suite> 